{
	"name": "williamstownsc",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"bin": {
		"wsc": "dist/cli/wsc.js"
	},
	"scripts": {
		"dev": "next dev --port 3003",
		"build": "npm run type:gen && next build",
		"build:sanity": "sanity build",
		"deploy:sanity": "sanity deploy",
		"start": "next start",
		"lint": "eslint",
		"lint-staged": "lint-staged",
		"format": "prettier . --write .",
		"format:check": "prettier . --check",
		"prepare": "husky",
		"type:gen": "sanity schema extract --path src/sanity/schema.json && sanity typegen generate",
		"type:check": "tsc --noEmit",
		"build:cli": "tsup",
		"dev:cli": "tsup --watch",
		"crawl:clubs": "npm run build:cli && node dist/cli/wsc.js crawl clubs",
		"crawl:clubs:ci": "npm run build:cli && xvfb-run --auto-servernum node dist/cli/wsc.js crawl clubs",
		"crawl:fixtures": "npm run build:cli && node dist/cli/wsc.js crawl fixtures",
		"crawl:fixtures:ci": "npm run build:cli && xvfb-run --auto-servernum node dist/cli/wsc.js crawl fixtures",
		"sync:clubs": "npm run build:cli && node dist/cli/wsc.js sync clubs",
		"sync:fixtures": "npm run build:cli && node dist/cli/wsc.js sync fixtures",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:report": "playwright show-report"
	},
	"dependencies": {
		"@aws-sdk/client-ses": "3.980.0",
		"@next/third-parties": "16.1.6",
		"@portabletext/toolkit": "5.0.1",
		"@sanity/client": "7.14.1",
		"@sanity/image-url": "2.0.3",
		"@sanity/vision": "5.7.0",
		"@tanstack/react-query": "5.90.20",
		"clsx": "2.1.1",
		"daisyui": "5.5.14",
		"date-fns": "4.1.0",
		"lucide-react": "0.563.0",
		"next": "16.1.6",
		"next-sanity": "12.0.16",
		"react": "19.2.4",
		"react-dom": "19.2.4",
		"react-hook-form": "7.71.1",
		"sanity": "5.7.0",
		"styled-components": "6.3.8",
		"zod": "4.3.6"
	},
	"devDependencies": {
		"@faker-js/faker": "^10.1.0",
		"@playwright/test": "1.58.1",
		"@tailwindcss/postcss": "^4",
		"@tanstack/react-query-devtools": "5.91.3",
		"@trivago/prettier-plugin-sort-imports": "^6.0.0",
		"@types/node": "^25.0.0",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"commander": "^14.0.2",
		"dotenv": "^17.2.3",
		"eslint": "^9",
		"eslint-config-next": "16.1.6",
		"eslint-config-prettier": "^10.0.0",
		"husky": "^9.0.7",
		"lint-staged": "^16.2.6",
		"playwright-core": "1.58.1",
		"prettier": "^3",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"tailwindcss": "^4",
		"tsup": "^8.5.1",
		"tsx": "^4.20.6",
		"typescript": "^5"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json}": [
			"prettier . --write",
			"eslint ."
		]
	},
	"volta": {
		"node": "24.12.0"
	},
	"browserslist": "> 1%"
}
